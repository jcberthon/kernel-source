 };
 MODULE_DEVICE_TABLE(pci, aac_pci_tbl);
@@ -238,7 +237,6 @@ static struct aac_driver_ident aac_drive
 	{ aac_src_init, "aacraid", "ADAPTEC ", "RAID            ", 2, AAC_QUIRK_SRC }, /* Adaptec PMC Series 6 (Tupelo) */
